{application, queuepusherl, [
	{description, "Pushes messages from RabbitMQ to either SMTP or HTTP servers"},
	{vsn, "0.1.0"},
	{id, "queuepusherl"},
	{env, [{rabbitmq_configs, [[
								{username, <<"guest">>},
								{password, <<"guest">>},
								{vhost, <<"/">>},
								{host, "localhost"},
								{port, 5672}
							   ]]},
		   {rabbitmq_work_queue, <<"qpush.work.queue">>},
		   {rabbitmq_work_exchange, <<"qpush.exchange">>},
		   {rabbitmq_retry_queue, <<"qpush.retry.queue">>},
		   {rabbitmq_retry_exchange, <<"qpush.dlx">>},
		   {rabbitmq_routing_key, <<"queuepusherl">>},
		   {smtp_retry_count, 10},
		   {smtp, [{event_retry_count, 10},
				   {event_initial_delay, 2000}]},
		   {http, [{event_retry_count, 10},
				   {event_initial_delay, 2000}]},
		   {error_smtp, []}
		   ]},
	{modules, []},
	{registered, []},
	{applications, [
		kernel,
		stdlib,
		inets,
		lager,
		ssl,
		gen_smtp,
		cowlib,
		jiffy,
		amqp_client,
		rabbit_common
	]},
	{mod, {qpusherl_app, []}}
]}.
